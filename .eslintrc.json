{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "semi": [2, "never"],
    "no-underscore-dangle": "off",
    "import/extensions": "off",
    "no-unused-vars": "off"
  }
}
